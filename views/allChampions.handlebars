
<div class="galleryContainer">
    <H1 class="galleryTitle">YOUR CHAMPION AWAITS!!!</H1>
    <div class="gallery testText">
            <!-- this is where the champion thumbnails are generated by the js code -->
            {{#each champions}}
                <div id="{{id}}">
                    <img src="{{mainPic}}" alt="champion thumbnail" class="thumbnail" title="{{name}}">
                </div>
            {{/each}}
    </div>
</div>